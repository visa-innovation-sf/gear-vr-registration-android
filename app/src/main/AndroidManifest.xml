<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="userregistration.gearvr.visa.com.userregistration">

    <!-- <uses-sdk -->
    <!-- android:minSdkVersion="10" -->
    <!-- android:targetSdkVersion="19" /> -->

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />

    <uses-feature
        android:name="android.hardware.camera.front"
        android:required="false" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/launcher"
        android:label="Gear VR"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activitites.UserRegistration">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activitites.UserSelection" />
        <activity android:name=".Activitites.VoiceToTextConverter"></activity>
    </application>

</manifest>